        {
          "inlineContent" : [
            {
              "text" : "The values are sampled uniformly in the range.  An optional shape can be used to broadcast into",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "a larger array.  An optional ",
              "type" : "text"
            },
            {
              "code" : "Key",
              "type" : "codeVoice"
            },
            {
              "text" : " can be specified to control the PRNG.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let key = MLXRandom.key(0)",
            "",
            "\/\/ create an array of shape [50] type Float values in the range 0 ..< 10",
            "let array = MLXRandom.uniform(0 ..< 10, [50], key: key)",
            "",
            "\/\/ same, but in range 0.5 ..< 1",
            "let array = MLXRandom.uniform(0.5 ..< 1, [50], key: key)"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        }
