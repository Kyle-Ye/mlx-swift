        {
          "code" : [
            "\/\/ a nonsensical Sequential layer, but it demonstrates how",
            "\/\/ to constructuct something with an interesting structure",
            "let b = Bool.random()",
            "let s = Sequential {",
            "    Tanh()",
            "    if b {",
            "        Tanh()",
            "    } else {",
            "        Sigmoid()",
            "    }",
            "    for _ in 0 ..< 3 {",
            "        Linear(10, 20)",
            "    }",
            "}"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "produces:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "Sequential {",
            "  layers: [",
            "    Tanh,",
            "    Sigmoid,",
            "    Linear(inputDimensions=10, outputDimensions=20, bias=true),",
            "    Linear(inputDimensions=10, outputDimensions=20, bias=true),",
            "    Linear(inputDimensions=10, outputDimensions=20, bias=true)",
            "  ],",
            "}"
          ],
