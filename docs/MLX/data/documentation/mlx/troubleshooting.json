  "primaryContentSections" : [
    {
      "content" : [
        {
          "anchor" : "Installation",
          "level" : 2,
          "text" : "Installation",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "See ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/mlx.swift.mlx\/documentation\/MLX\/install",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " for more information.  If you use Xcode or SwiftPM to reference",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "the package on github.com there should be no manual steps.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "If you clone the ",
              "type" : "text"
            },
            {
              "code" : "mlx-swift",
              "type" : "codeVoice"
            },
            {
              "text" : " repository yourself, e.g. in order to",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "work on the implementation please be aware that it uses git submodules.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "When you clone the repository you must give an argument to check out the submodules at the same time:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "git clone --recurse-submodules https:\/\/github.com\/ml-explore\/mlx-swift.git"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "If you have already cloned the repository you can use this command to force the submodules to update:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "git submodule update --init --recursive"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "anchor" : "Running-From-Command-Line",
          "level" : 2,
          "text" : "Running From Command Line",
          "type" : "heading"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "MLX requires metal shaders from the ",
                  "type" : "text"
                },
                {
                  "code" : "Cmlx",
                  "type" : "codeVoice"
                },
                {
                  "text" : " framework – these are not usable",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "from command line tools unless ",
                  "type" : "text"
                },
                {
                  "code" : "DYLD_FRAMEWORK_PATH",
                  "type" : "codeVoice"
                },
                {
                  "text" : " makes them visible.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Note",
          "style" : "note",
          "type" : "aside"
        },
        {
          "inlineContent" : [
            {
              "identifier" : "doc:\/\/mlx.swift.mlx\/documentation\/MLX",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " is built on top of the ",
              "type" : "text"
            },
            {
              "code" : "mlx",
              "type" : "codeVoice"
            },
            {
              "text" : " C++ library packaged in the ",
              "type" : "text"
            },
            {
              "code" : "Cmlx",
              "type" : "codeVoice"
            },
            {
              "text" : " swift package.  ",
              "type" : "text"
            },
            {
              "code" : "Cmlx",
              "type" : "codeVoice"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "produces a bundle called ",
              "type" : "text"
            },
            {
              "code" : "mlx-swift_Cmlx.bundle",
              "type" : "codeVoice"
            },
            {
              "text" : " which contains the compiled metal shaders.",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "If you build a command line tool and run it from Xcode, the ",
              "type" : "text"
            },
            {
              "code" : "DYLD_FRAMEWORK_PATH",
              "type" : "codeVoice"
            },
            {
              "text" : " is set",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "so that this bundle is found.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "If you want to run the same command from the shell you must manually set the ",
              "type" : "text"
            },
            {
              "code" : "DYLD_FRAMEWORK_PATH",
              "type" : "codeVoice"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "to the build directory.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "Note: applications will link the ",
              "type" : "text"
            },
            {
              "code" : "MLX",
              "type" : "codeVoice"
            },
            {
              "text" : " and ",
              "type" : "text"
            },
            {
              "code" : "Cmlx",
              "type" : "codeVoice"
            },
            {
              "text" : " libraries which will automatically",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "provide access to the metal libraries as a resource of your application.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "anchor" : "Building",
          "level" : 2,
          "text" : "Building",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Specific build issues you may encounter.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "anchor" : "The-file-kernels-couldnt-be-opened-",
          "level" : 3,
          "text" : "The file “kernels” couldn’t be opened …",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "This message comes from part of the build process that prepares the metal kernels.",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "This may mean that the ",
              "type" : "text"
            },
            {
              "code" : "kernels",
              "type" : "codeVoice"
            },
            {
              "text" : " directory in ",
              "type" : "text"
            },
            {
              "code" : "Source\/Cmlx\/mlx\/mlx\/backend\/metal\/kernels",
              "type" : "codeVoice"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "does not exist.  In particular it may mean that the git submodules were not checked out – see “Installation” above.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "anchor" : "received-multiple-target-ended-messages-for-target-ID-",
          "level" : 3,
          "text" : "received multiple target ended messages for target ID …",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "If you receive a message like this:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "error: Internal inconsistency error: received multiple target ended messages for target ID '5' or received target ended message but did not receive corresponding target started message, while retrieving parent activity in taskStarted message."
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "There are a few approaches that have been observed to work around the issue:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "wait a few seconds and try building again",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "quit and restart xcode",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "clean the build folder and rebuild",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "use Xcode 15.3 beta 2 or later",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        },
        {
          "anchor" : "Porting-Python-Code",
          "level" : 2,
          "text" : "Porting Python Code",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "See ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/mlx.swift.mlx\/documentation\/MLX\/converting-python",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " for examples and information about",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "symbols changes from python to swift.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        }
      ],
      "kind" : "content"
    }
